{
  "name": "aoc-2024",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:wasm && tsc && vite build",
    "build:wasm": "cd aoc-core && wasm-pack build --target web --out-dir ../src/pkg",
    "dev": "npm-run-all --parallel watch:rust dev:vite",
    "dev:vite": "vite",
    "watch:rust": "cd aoc-core && cargo watch -i .gitignore -s \"wasm-pack build --target web --out-dir ../src/pkg\""
  },
  "dependencies": {
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.6",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  }
}
